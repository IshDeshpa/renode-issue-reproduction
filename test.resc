$name?="stm32"

using sysbus
mach create $name
machine LoadPlatformDescription @stm32f413.repl

$bin?=@artifacts/output.elf

showAnalyzer usart2

macro reset
"""
    sysbus LoadELF $bin
"""
runMacro $reset
